{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "ws-adf-proyecto-final-de-azure-taylor-asprilla"
		},
		"ls_adls_proyecto_final_de_azure_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"ls_adls_proyecto_final_de_azure\""
		},
		"ls_fs_proyecto_final_de_azure_password": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"password\"de \"ls_fs_proyecto_final_de_azure\""
		},
		"ls_adls_proyecto_final_de_azure_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsproyectofinaldeazure.dfs.core.windows.net/"
		},
		"ls_fs_proyecto_final_de_azure_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\Taylor\\Cursos\\Ingenier√≠a de datos con Azure Microsoft\\Clase 5\\Entrega #2\\data"
		},
		"ls_fs_proyecto_final_de_azure_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Taylor"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ws-adf-proyecto-final-de-azure-taylor-asprilla_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_proyecto_final_de_azure_accountKey": {
						"value": "[parameters('ls_adls_proyecto_final_de_azure_accountKey')]"
					},
					"ls_fs_proyecto_final_de_azure_password": {
						"value": "[parameters('ls_fs_proyecto_final_de_azure_password')]"
					},
					"ls_adls_proyecto_final_de_azure_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_proyecto_final_de_azure_properties_typeProperties_url')]"
					},
					"ls_fs_proyecto_final_de_azure_properties_typeProperties_host": {
						"value": "[parameters('ls_fs_proyecto_final_de_azure_properties_typeProperties_host')]"
					},
					"ls_fs_proyecto_final_de_azure_properties_typeProperties_userId": {
						"value": "[parameters('ls_fs_proyecto_final_de_azure_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}